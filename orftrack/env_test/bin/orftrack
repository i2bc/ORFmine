#!/home/nicolas/ORFmine/orftrack/env_test/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'orftrack','console_scripts','orftrack'
__requires__ = 'orftrack'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('orftrack', 'console_scripts', 'orftrack')()
    )
